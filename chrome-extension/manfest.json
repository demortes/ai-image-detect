{
    "manifest_version": 3,
    "name": "AI Image Detection",
    "description": "Simple extension to detect AI images.",
    "version": "1.0",
    "icons": {
        "16": "images/icon-16.jpg",
        "32": "images/icon-32.jpg",
        "48": "images/icon-48.jpg",
        "128": "images/icon-128.jpg"
    },
    "content_scripts": [
        {
            "js": [
                "scripts/content.js"
            ],
            "matches": [
                "https://facebook.com/*",
                "https://www.facebook.com/*",
                "https://m.facebook.com/*"
            ]
        }
    ]
}